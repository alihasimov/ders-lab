def tek_indeks_ortalama(C):
    tek_indeksli = [C[i] for i in range(1, len(C), 2)]
    if tek_indeksli:
        return sum(tek_indeksli) / len(tek_indeksli)
    else:
        return None

C = [5, 10, 15, 20, 25, 30]
orta = tek_indeks_ortalama(C)
print("Tək indeksli elementlərin hesabi ortası:", orta)
